{{!-- Put quotes around attributes that may have spaces, or handlebars will 
      only output the first word. --}}
<div class="row">	
	<div class="large-12 columns">
		<h2><i class="fi-torsos-all"></i> All Patients (<strong><em>{{patients.length}}</em></strong>)</h2>
	</div>
</div>
<div class="row">
	<div class="large-12 large-centered columns">
		<table class="large-12">
			<thead>
				<tr>
					<th>Patient Alias</th>
					<th>ID</th>
					<th>Sex</th>
					<th>Age</th>
					<th>Date Uploaded</th>
				</tr>
			</thead>
			<tbody>
				{{#each patients}}
				<tr class="patient-row">
					<td>{{patient_alias}}</a></td>
					<td class="frangipani-patient-id">{{patient_id}}</td>
					<td>{{sex}}</td>
					<td>{{age}}</td>
					<td>{{completed}}</td>
				</tr>
				{{/each}}
			</tbody>
		</table>
	</div>	
</div>